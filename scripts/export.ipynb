						"# ../go/ossekaiserver/internal/ginapi/authmiddleware.go\n",
						"```\n",
						"package ginapi\n",
						"\n",
						"import (\n",
						"\t\"log\"\n",
						"\t\"net/http\"\n",
						"\t\"ossekaiserver/internal/auth\"\n",
