     "text": [
      "\n",
      "# ../apps/noteprez/noteprez/template/__init__.py\n",
      "```\n",
      "from .factory import *\n",
      "\n",
      "```\n",
      "\n",
      "# ../apps/noteprez/noteprez/template/factory.py\n",
      "```\n",
      "from typing import Protocol, List, Any, Dict\n",
      "\n",
      "\n",
      "# プロトコルの定義\n",
      "class ExampleRepository(Protocol):\n",
      "    def read(self) -> List[Any]:\n",
      "        \"\"\"\n",
      "        Exampleを読み込む\n",
      "        具体的なデータ型やフォーマットは実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "class ExampleParser(Protocol):\n",
      "    def parse(self, examples: List[Any]) -> Dict:\n",
      "        \"\"\"\n",
      "        Exampleを解析してテンプレートデータを生成する\n",
      "        具体的なデータ型やフォーマットは実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "class TemplateRepository(Protocol):\n",
      "    def save(self, template_data: Dict):\n",
      "        \"\"\"\n",
      "        テンプレートデータを保存する\n",
      "        保存方法（ファイル、データベース等）は実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "# 実装クラス\n",
      "class Converter:\n",
      "    \"\"\"\n",
      "    examplesを解析してテンプレートに変換する\n",
      "    \"\"\"\n",
      "\n",
      "    def __init__(self, example_parser: ExampleParser, template_repo: TemplateRepository):\n",
      "        self.example_parser = example_parser\n",
      "        self.template_repo = template_repo\n",
      "\n",
      "    def convert(self, examples: List[Any]) -> Dict:\n",
      "        template_data = self.example_parser.parse(examples)\n",
      "        self.template_repo.save(template_data)\n",
      "        return template_data\n",
      "\n",
      "\n",
      "class Factory:\n",
      "    \"\"\"\n",
      "    テンプレートの元となるexamplesを読み込み\n",
      "    それらを解析してテンプレートに変換する\n",
      "    \"\"\"\n",
      "\n",
      "    def __init__(self, example_repo: ExampleRepository, converter: Converter):\n",
      "        self.example_repo = example_repo\n",
      "        self.converter = converter\n",
      "\n",
      "    def create(self) -> Dict:\n",
      "        examples = self.example_repo.read()\n",
      "        template_data = self.converter.convert(examples)\n",
      "        return template_data\n",
      "\n",
      "```\n",
      "\n",
      "# ../apps/noteprez/noteprez/documentation/__init__.py\n",
      "```\n",
      "from .engine import *\n",
      "```\n",
      "\n",
      "# ../apps/noteprez/noteprez/documentation/engine.py\n",
      "```\n",
      "from typing import Protocol, Any, Dict\n",
      "\n",
      "\n",
      "# プロトコルの定義\n",
      "class DataMapper(Protocol):\n",
      "    def map(self, extracted_data: Any, template_data: Dict) -> Dict:\n",
      "        \"\"\"\n",
      "        抽出したデータをテンプレートにマッピングする\n",
      "        具体的なデータ型やフォーマットは実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "class OutputConverter(Protocol):\n",
      "    def convert(self, mapped_data: Dict) -> Any:\n",
      "        \"\"\"\n",
      "        マッピングされたデータを指定された出力形式に変換する\n",
      "        具体的なデータ型やフォーマットは実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "class OutputRepository(Protocol):\n",
      "    def save(self, output_data: Any):\n",
      "        \"\"\"\n",
      "        生成されたドキュメントを保存する\n",
      "        保存方法（ファイル、データベース等）は実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "# 実装クラス\n",
      "class Engine:\n",
      "    \"\"\"\n",
      "    DataMapperでデータを取得し、OutputConverterで変換\n",
      "    OutputRepositoryに保存する\n",
      "    \"\"\"\n",
      "\n",
      "    def __init__(self, data_mapper: DataMapper, output_converter: OutputConverter, output_repo: OutputRepository):\n",
      "        self.data_mapper = data_mapper\n",
      "        self.output_converter = output_converter\n",
      "        self.output_repo = output_repo\n",
      "\n",
      "    def generate(self, extracted_data: Any, template_data: Dict):\n",
      "        mapped_data = self.data_mapper.map(extracted_data, template_data)\n",
      "        output_data = self.output_converter.convert(mapped_data)\n",
      "        self.output_repo.save(output_data)\n",
      "\n",
      "```\n",
      "\n",
      "# ../apps/noteprez/noteprez/content/__init__.py\n",
      "```\n",
      "from .extractor import *\n",
      "\n",
      "```\n",
      "\n",
      "# ../apps/noteprez/noteprez/content/extractor.py\n",
      "```\n",
      "from typing import Protocol, Any, List, Dict\n",
      "\n",
      "\n",
      "# プロトコルの定義\n",
      "class NoteRepository(Protocol):\n",
      "    def read(self) -> Any:\n",
      "        \"\"\"\n",
      "        ノートブックデータを読み込む\n",
      "        具体的なデータ型やフォーマットは実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "class AssetExtractor(Protocol):\n",
      "    def extract(self, note_data: Any) -> List[Any]:\n",
      "        \"\"\"\n",
      "        ノートデータからAssetデータを抽出する\n",
      "        具体的なデータ型やフォーマットは実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "class TextExtractor(Protocol):\n",
      "    def extract(self, note_data: Any) -> List[Any]:\n",
      "        \"\"\"\n",
      "        ノートデータからテキストデータを抽出する\n",
      "        具体的なデータ型やフォーマットは実装に依存\n",
      "        \"\"\"\n",
      "        ...\n",
      "\n",
      "\n",
      "# 実装クラス\n",
      "class Extractor:\n",
      "    \"\"\"\n",
      "    ノートデータ抽出、NoteRepositoryからノートを読みこみ\n",
      "    ContentAnalyzerでテキストやAssetを解析、抽出する\n",
      "    \"\"\"\n",
      "\n",
      "    def __init__(self, note_repo: NoteRepository, content_analyzer: \"ContentAnalyzer\"):\n",
      "        self.note_repo = note_repo\n",
      "        self.content_analyzer = content_analyzer\n",
      "\n",
      "    def extract(self) -> Dict:\n",
      "        note_data = self.note_repo.read()\n",
      "        return self.content_analyzer.analyze(note_data)\n",
      "\n",
      "\n",
      "class ContentAnalyzer:\n",
      "    \"\"\"\n",
      "    ノートデータ解析\n",
      "    AssetExtractorとTextExtractorを使ってノートデータを解析する\n",
      "    \"\"\"\n",
      "\n",
      "    def __init__(self, asset_extractor: AssetExtractor, text_extractor: TextExtractor):\n",
      "        self.asset_extractor = asset_extractor\n",
      "        self.text_extractor = text_extractor\n",
      "\n",
      "    def analyze(self, note_data: Any) -> Dict:\n",
      "        assets = self.asset_extractor.extract(note_data)\n",
      "        text = self.text_extractor.extract(note_data)\n",
      "        return {\n",
      "            \"assets\": assets,\n",
      "            \"text\": text\n",
      "        }\n",
      "\n",
      "```\n",
      "\n",
      "# ../apps/noteprez/cmd/noteprez.py\n",
      "```\n",
      "#!/usr/bin/env python\n",
      "\n",
      "from noteprez import content\n",
      "from noteprez import documentation\n",
      "from noteprez import template\n",
      "from typing import List, Any, Dict\n",
      "\n",
      "\n",
      "# template モジュール用のモック\n",
      "class MockExampleRepository(template.ExampleRepository):\n",
      "    def read(self) -> List[Any]:\n",
      "        raise NotImplementedError\n",
      "\n",
      "\n",
      "class MockExampleParser(template.ExampleParser):\n",
      "    def parse(self, examples: List[Any]) -> Dict:\n",
      "        return {\"template_name\": \"template1\", \"content\": \"This is a template.\"}\n",
      "\n",
      "\n",
      "class MockTemplateRepository(template.TemplateRepository):\n",
      "    def save(self, template_data: Dict):\n",
      "        print(\"Template saved:\", template_data)\n",
      "\n",
      "\n",
      "# content モジュール用のモック\n",
      "class MockNoteRepository(content.NoteRepository):\n",
      "    def read(self) -> Any:\n",
      "        return \"Note Content\"\n",
      "\n",
      "\n",
      "class MockAssetExtractor(content.AssetExtractor):\n",
      "    def extract(self, note_data: Any) -> List[Any]:\n",
      "        return [\"Asset 1\", \"Asset 2\"]\n",
      "\n",
      "\n",
      "class MockTextExtractor(content.TextExtractor):\n",
      "    def extract(self, note_data: Any) -> List[str]:\n",
      "        return [\"Text 1\", \"Text 2\"]\n",
      "\n",
      "\n",
      "# documentation モジュール用のモック\n",
      "class MockDataMapper(documentation.DataMapper):\n",
      "    def map(self, extracted_data: Any, template_data: Dict) -> Dict:\n",
      "        return {\"title\": extracted_data[\"text_elements\"][0].content, \"content\": template_data[0][\"content\"]}\n",
      "\n",
      "\n",
      "class MockOutputConverter(documentation.OutputConverter):\n",
      "    def convert(self, mapped_data: Dict) -> Any:\n",
      "        return f\"Title: {mapped_data['title']}\\nContent: {mapped_data['content']}\"\n",
      "\n",
      "\n",
      "class MockOutputRepository(documentation.OutputRepository):\n",
      "    def save(self, output_data: Any):\n",
      "        print(\"Document saved:\", output_data)\n",
      "\n",
      "\n",
      "# PoCコード\n",
      "# template モジュールのオブジェクトを作成\n",
      "example_repo = MockExampleRepository()\n",
      "example_parser = MockExampleParser()\n",
      "template_repo = MockTemplateRepository()\n",
      "converter = template.Converter(example_parser, template_repo)\n",
      "factory = template.Factory(example_repo, converter)\n",
      "\n",
      "# テンプレートを生成\n",
      "template_data = factory.create()\n",
      "\n",
      "# content モジュールのオブジェクトを作成\n",
      "note_repo = MockNoteRepository()\n",
      "asset_extractor = MockAssetExtractor()\n",
      "text_extractor = MockTextExtractor()\n",
      "content_analyzer = content.ContentAnalyzer(asset_extractor, text_extractor)\n",
      "extractor = content.Extractor(note_repo, content_analyzer)\n",
      "\n",
      "# ノートからデータを抽出\n",
      "extracted_data = extractor.extract()\n",
      "\n",
      "# documentation モジュールのオブジェクトを作成\n",
      "data_mapper = MockDataMapper()\n",
      "output_converter = MockOutputConverter()\n",
      "output_repo = MockOutputRepository()\n",
      "engine = documentation.Engine(data_mapper, output_converter, output_repo)\n",
      "\n",
      "# ドキュメントを生成\n",
      "engine.generate(extracted_data, template_data)\n",
      "\n",
      "```\n"
     ]
    },
