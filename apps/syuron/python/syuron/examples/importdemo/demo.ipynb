						"    \"# 3階のtensorを定義\\n\",\n",
						"    \"\\n\",\n",
						"    \"\\n\",\n",
						"    \"def create_tensor():\\n\",\n",
						"    \"    T = np.zeros((3, 3, 3))\\n\",\n",
						"    \"    for i in range(3):\\n\",\n",
						"    \"        for j in range(3):\\n\",\n",
